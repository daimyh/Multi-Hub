local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/daimyh/RobloxGui/main/source'))()

local Window = Rayfield:CreateWindow({
	Name = "Multi Hub",
	LoadingTitle = "Multi Hub",
	LoadingSubtitle = "by Daimy",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "MultiHubPSX"
	},
        Discord = {
        	Enabled = true,
        	Invite = "9haMu9YWsT", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = false -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "multi Hub",
		Subtitle = "Key System",
		Note = "Multi Hub",
		FileName = "PSXMultiHub",
		SaveKey = false,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Beta"
	}
})

Rayfield:Notify({
   Title = "Note!",
   Content = "Not all of these scripts where made by us. all where tested and are safe and working",
   Duration = 6.5,
   Image = 4483362458,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Okay!",
         Callback = function()
         print("hi")
      end
   },
},
})

-- psx
local PsxTab = Window:CreateTab("Pet Simulator X", 11347112419) -- Title, Image
local PsxSection = PsxTab:CreateSection("Scripts")
local Button = PsxTab:CreateButton({
   Name = "Button Example",
   Callback = function()
   -- The function that takes place when the button is pressed
   end,
})
